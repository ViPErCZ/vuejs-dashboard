        "express": "~4.18.1",
        "gaugeJS": "^1.3.7",
        "http-errors": "~2.0.0",
        "ionicons": "^4.5.6",
        "jquery": "^3.6.0",
        "morgan": "~1.9.1",
        "normalize.css": "^8.0.1",
        "pug": "^3.0.2",
        "socket.io": "^4.5.2",
        "socket.io-client": "^4.5.2",
        "systeminformation": "^5.12.6",
        "vue": "^3.2.40",
        "vue-socket.io-extended": "^5.0.0-alpha.5",
        "vuex": "^4.0.2",
        "@fortawesome/fontawesome-svg-core": "^6.2.0",
        "@fortawesome/vue-fontawesome": "^3.0.1",
